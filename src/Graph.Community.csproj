<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/graspex/graspex</PackageProjectUrl>
    <PackageIconUrl>https://github.com/graspex/graspex/raw/master/docs/graspex-nuget.png</PackageIconUrl>
    <RepositoryUrl>https://github.com/graspex/graspex</RepositoryUrl>
    <PackageTags>graph sharepoint dotnetcore</PackageTags>
    <Description>Extensions to Graph SDK for SharePoint REST API</Description>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="Requests\Site\IChangeLogCollectionPage.cs" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Graph" Version="1.15.*" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="Microsoft.Graph">
      <HintPath>..\..\..\MicrosoftGraph\msgraph-sdk-dotnet\src\Microsoft.Graph\bin\Debug\net45\Microsoft.Graph.dll</HintPath>
    </Reference>
  </ItemGroup>

</Project>
